rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /natal/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null
        && (request.resource.contentType.matches('image/.*'))
        && request.resource.size < 10 * 1024 * 1024;
    }
  }
}
